<% include partials/header.ejs %>
<div class="container">
  <h1>Previously Searched Keywords</h1>
  <% if(keywords !== null) { for (let i = keywords.length-1; i>=0; i--) { %>
  <div class="container">
    <ul class="list-group row">
      <li class="list-group-item col-xs-6">
        <a href="/<%= keywords[i].title %>"><%= keywords[i].title %></a>
      </li>
    </ul>
  </div>
  <% }} %>
</div>
</body>
</html>
